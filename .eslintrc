{
  "globals": {
    "require": true,
    "exports": true,
    "module": true,
    "Future": true,
    "describe": true,
    "it": true,
    "before": true,
    "after": true,
    "beforeEach": true,
    "afterEach": true,
    "setTimeout": true,
    "clearTimeout": true,
    "setInterval": true,
    "Packages": true,
    "clearInterval": true,
    "importClass": true,
    "java": true
  },
  "rules": {
    "quotes": false,
    "strict": false,
    "space-before-blocks": [2, "always"],
    "brace-style": [2, "1tbs", {"allowSingleLine": true}],
    "spaced-line-comment": [2, "always"],
    "radix": [2, "always"],
    "key-spacing": [2, "always", {"beforeColon": false, "afterColon": true}],
    "no-multiple-empty-lines": [2, {max: 1}],
    "indent": [2, 2],
    "no-console": false,
    "new-cap": false,
    "no-shadow": false,
    "no-octal": false
  }
}
